{
    "extractor_type": "csv",
    "extractor_config": {
        "pretrained_model_name_or_path": "distilbert-base-uncased",
        "sentence_col": "text",
        "labels_col": "label",
        "cache_path": "./data/csv/",
        "read_cache": false
    },
    "url_or_paths": [
        "../bert_extras/covid_tweets/Corona_NLP_train.csv",
        "../bert_extras/fake_tweets/tweets500.csv"
    ],
    "problem_type": "russian_fake_tweets",
    "train_arguments": {
        "output_dir": "./results",
        "num_train_epochs": 1,
        "per_device_train_batch_size": 16,
        "per_device_eval_batch_size": 64,
        "warmup_steps": 100,
        "weight_decay": 0.01,
        "logging_dir": "./logs",
        "logging_steps": 10
    }
}